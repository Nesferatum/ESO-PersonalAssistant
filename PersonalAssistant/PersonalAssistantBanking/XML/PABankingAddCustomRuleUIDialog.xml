<GuiXml>
    <Controls>
        <TopLevelControl name="PABankingAddCustomRuleWindow" mouseEnabled="true" movable="true" clampedToScreen="true" hidden="true">
            <Dimensions x="500" y="400" />
            <Anchor point="CENTER" offsetY="-100"/>
            <Controls>

                <Backdrop name="$(parent)Bg" inherits="ZO_DefaultBackdrop"><AnchorFill/></Backdrop>

                <Button name="$(parent)CloseXButton" inheritAlpha="true" verticalAlignment="CENTER" horizontalAlignment="CENTER">
                    <Anchor point="CENTER" relativeTo="$(parent)" relativePoint="TOPRIGHT" offsetX="-20" offsetY="20" />
                    <Dimensions x="48" y="48" />
                    <Textures normal="/esoui/art/hud/radialicon_cancel_up.dds" mouseOver="/esoui/art/hud/radialicon_cancel_over.dds" />
                    <OnClicked>
                        PABankingAddCustomRuleWindow:SetHidden(true)
                    </OnClicked>
                </Button>

                <Label name="$(parent)Header" font="ZoFontAnnounceMedium" text="|cFFD700PA B|r|cFFFFFFanking|r: Add custom rule">
                    <Anchor point="TOPLEFT" offsetX="10" offsetY="10" />
                </Label>

                <Label name="$(parent)Description" font="ZoFontGame" text="Some Test Label">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)Header" relativePoint="BOTTOMLEFT" offsetY="10"/>
                </Label>

                <Control name="$(parent)BagDropdown" inherits="ZO_ComboBox">
                    <Anchor point="TOPLEFT" relativeTo="$(parent)Description" relativePoint="BOTTOMLEFT" offsetY="10"/>
                    <Dimensions x="100"/>
                    <OnInitialized>
                        ZO_ComboBox:New(self)
                    </OnInitialized>
                </Control>

                <Control name="$(parent)MathOperatorDropdown" inherits="ZO_ComboBox">
                    <Anchor point="LEFT" relativeTo="$(parent)BagDropdown" relativePoint="RIGHT" offsetX="10"/>
                    <Dimensions x="100"/>
                    <OnInitialized>
                        ZO_ComboBox:New(self)
                    </OnInitialized>
                </Control>

                <Backdrop name="$(parent)AmountEditBg" inherits="ZO_EditBackdrop">
                    <Anchor point="LEFT" relativeTo="$(parent)MathOperatorDropdown" relativePoint="RIGHT" offsetX="10"/>
                    <Dimensions x="100"/>
                    <Controls>
                        <EditBox name="$(parent)AmountEdit" inherits="ZO_DefaultEditForBackdrop ZO_EditDefaultText">
                        </EditBox>
                    </Controls>
                </Backdrop>

                <Label name="$(parent)IconX" font="ZoFontGame" text="x">
                    <Anchor point="LEFT" relativeTo="$(parent)AmountEditBg" relativePoint="RIGHT" offsetX="10"/>
                </Label>


                <Control name="$(parent)Item" mouseEnabled="true">
                    <Anchor point="LEFT" relativeTo="$(parent)IconX" relativePoint="RIGHT" offsetX="10"/>
                    <Dimensions x="64" y="64"/>
                    <Controls>
                        <Texture name="$(parent)ItemIcon" textureFile="esoui/art/icons/ava_siege_weapon_001.dds" mouseEnabled="true">
                            <Anchor point="TOPLEFT" relativePoint="TOPLEFT" />
                            <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" />
                        </Texture>
                    </Controls>
                    <OnMouseEnter>
                        <!--MasterMerchant:HeaderToolTip(self, GetString(SK_ITEM_TOOLTIP))-->
                        PersonalAssistant.CustomDialogs.showTooltip(self)
                    </OnMouseEnter>
                    <OnMouseExit>
                        ClearTooltip(InformationTooltip)
                    </OnMouseExit>
                </Control>

                <Button name="$(parent)_ButtonPostCombatStatic" inherits="ZO_DefaultButton">
                    <Anchor point="BOTTOMRIGHT" offsetX="-20" offsetY="-20"/>
                    <Controls>
                        <Label name="$(parent)TextAgain" font="ZoFontGame" text="Some Test Button">
                            <Anchor point="CENTER" />
                        </Label>
                    </Controls>
                </Button>

            </Controls>

        </TopLevelControl>
    </Controls>
</GuiXml>


